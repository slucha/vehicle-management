{% extends "FOSUserBundle::layout.html.twig" %} {% block
fos_user_content %} {% if error %}
<div class="alert">
	<button type="button" class="close" data-dismiss="alert">Ã—</button>
	<strong>Erreur!</strong> {{
	error|trans({},'VehicleManagementUserBundle') }}
</div>
{% endif %}
<div class="row">
	<div class="span3">
		<form class="well" action="{{ path("fos_user_security_check") }}" method="post">
			<input type="hidden" name="_csrf_token" value="{{ csrf_token }}" /> <label
				for="username">{{ 'security.login.username'|trans({}, 'FOSUserBundle')
				}}</label> <input type="text" class="input-small" id="username" name="_username"
				value="{{ last_username }}" /> <label for="password">{{
				'security.login.password'|trans({}, 'FOSUserBundle') }}</label> <input
				type="password" class="input-small" id="password" name="_password" /> 
				<input type="submit" id="_submit" name="_submit"
				value="{{ 'security.login.submit'|trans({}, 'FOSUserBundle') }}" />
		</form>
	</div>
</div>
{% endblock fos_user_content %}
